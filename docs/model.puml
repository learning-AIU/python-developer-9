@startuml

hide abstract empty members


abstract class int
abstract class tuple


class Token {
    -{static}minimum: <i>int</i> = 1
    -{static}maximum: <i>int</i> = 90
    +strike: <i>bool</i> = False
    __new__()
    __init__()
    __str__()
}


class Row {
    -{static}cells: <i>int</i> = 9
    -{static}tokens: <i>int</i> = 5
    __init__()
    __str__()
}


class Card {
    -{static}rows: <i>int</i> = 3
    +tokens_set: <i>set</i>
    __init__()
    __str__()
}


int <|--- Token
tuple <|-- Row
tuple <|-- Card

Row o-- Token
Card *-right- Row
Card o-- Token


@enduml